<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Button">
        <Setter Property="Height" Value="38"></Setter>
        <Setter Property="Padding" Value="10"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
        <Setter Property="FontFamily" Value="Tw Cen MT"></Setter>
        <Setter Property="FontSize" Value="14"></Setter>
        <Setter Property="Background" Value="#FF9C1A"></Setter>
        <Setter Property="Foreground" Value="#37474F"></Setter>
    </Style>

    <Style TargetType="TextBox">
        <Setter Property="Height" Value="38"></Setter>
        <Setter Property="Padding" Value="10"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
        <Setter Property="FontFamily" Value="Tw Cen MT"></Setter>
        <Setter Property="FontSize" Value="14"></Setter>
        <Setter Property="Foreground" Value="#37474F"></Setter>
    </Style>

    <Style TargetType="PasswordBox">
        <Setter Property="Height" Value="38"></Setter>
        <Setter Property="Padding" Value="10"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
        <Setter Property="FontFamily" Value="Tw Cen MT"></Setter>
        <Setter Property="FontSize" Value="14"></Setter>
        <Setter Property="Foreground" Value="#37474F"></Setter>
    </Style>

    <Style TargetType="ComboBox">
        <Setter Property="Height" Value="38"></Setter>
        <Setter Property="Padding" Value="10"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
        <Setter Property="FontFamily" Value="Tw Cen MT"></Setter>
        <Setter Property="FontSize" Value="14"></Setter>
        <Setter Property="Foreground" Value="#37474F"></Setter>
    </Style>

    <Style TargetType="DatePicker">
        <Setter Property="Height" Value="38"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
        <Setter Property="FontFamily" Value="Tw Cen MT"></Setter>
        <Setter Property="FontSize" Value="14"></Setter>
        <Setter Property="Foreground" Value="#37474F"></Setter>
    </Style>

    <Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="#78909C"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="FontFamily" Value="Tw Cen MT"></Setter>
        <Setter Property="FontSize" Value="14"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
    </Style>

    <Style TargetType="DataGrid">
        <Setter Property="Foreground" Value="#78909C"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
        <Setter Property="Padding" Value="10"></Setter>
    </Style>

    <Style TargetType="Image">
        <Setter Property="Stretch" Value="Uniform"></Setter>
        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality"></Setter>
        <Setter Property="Margin" Value="20"></Setter>
    </Style>
</ResourceDictionary>